for my $loc (@msa) {
  open FH,"$loc.txt";
  my @fips = ();
  while (my $line = <FH>) {
    chomp $line;
    push @fips,$line;
  }
  print "$loc";
  print "\t$_" for @fips;
  print "\n";
}
exit;
